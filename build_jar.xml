<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project passiveAggressor">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="${dir.buildfile}"/>
    <property name="dir.jarfile" value="${dir.workspace}/bin"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/passiveAggressor/bin/passiveAggressor.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="passiveAggressor.PassiveAggressor"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.workspace}/passiveAggressor/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.workspace}/passiveAggressor/jnetpcap-1.4.r1425/jnetpcap.jar"/>
        </jar>
    </target>
</project>
